Cell[TextData[{
 "Let\[CloseCurlyQuote]s define a coordinate transformation from a set of \
coordinates ",
 Cell[BoxData[
  FormBox[
   OverscriptBox["x", "\[RightVector]"], TraditionalForm]]],
 " to ",
 Cell[BoxData[
  FormBox[
   OverscriptBox["y", "\[RightVector]"], TraditionalForm]]],
 " as:"
}], "Text",
