Cell[TextData[{
 "Note that for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Alpha]", "=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"d", "-", "1"}], ")"}], "/", "2"}]}], TraditionalForm]]],
 ", we have: ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{"\[Alpha]", " ", 
      RowBox[{"(", 
       RowBox[{"\[Alpha]", "-", "d", "+", "2"}], ")"}]}], 
     SuperscriptBox["R", "2"]], "=", 
    RowBox[{
     FractionBox[
      RowBox[{
       RowBox[{"(", 
        RowBox[{"d", "-", "1"}], ")"}], 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"d", "/", "2"}], "-", 
         RowBox[{"1", "/", "2"}], "-", "d", "+", "2"}], ")"}]}], 
      RowBox[{"2", 
       SuperscriptBox["R", "2"]}]], "=", 
     RowBox[{
      FractionBox[
       RowBox[{
        RowBox[{"(", 
         RowBox[{"d", "-", "1"}], ")"}], 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"3", "/", "2"}], "-", 
          RowBox[{"d", "/", "2"}]}], ")"}]}], 
       RowBox[{"2", 
        SuperscriptBox["R", "2"]}]], "=", 
      RowBox[{"-", 
       FractionBox[
        RowBox[{
         RowBox[{"(", 
          RowBox[{"d", "-", "1"}], ")"}], 
         RowBox[{"(", 
          RowBox[{"d", "-", "3"}], ")"}]}], 
