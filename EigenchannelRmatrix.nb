Cell["\<\
In general, one may remove 1st derivative terms from the Hamiltonian by \
factoring out some R-dependence from the wavefunction.  One usually writes\
\>", "Text",
 CellChangeTimes->{{3.763476730390414*^9, 3.7634767830418053`*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"\[CapitalPsi]", "(", 
    RowBox[{"R", ",", "\[CapitalOmega]"}], ")"}], "=", 
   RowBox[{
    SuperscriptBox["R", 
     RowBox[{
      RowBox[{"-", 
       RowBox[{"(", 
        RowBox[{"d", "-", "1"}], ")"}]}], "/", "2"}]], 
    RowBox[{"\[Psi]", "(", 
     RowBox[{"R", ",", "\[CapitalOmega]"}], ")"}]}]}], 
  TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.763476785515357*^9, 3.763476821385248*^9}}],

Cell["but we will write", "Text",
 CellChangeTimes->{{3.763476822690566*^9, 3.763476835488158*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"\[CapitalPsi]", "(", 
    RowBox[{"R", ",", "\[CapitalOmega]"}], ")"}], "=", 
   RowBox[{
    SuperscriptBox["R", 
     RowBox[{"-", "\[Alpha]"}]], 
    RowBox[{"\[Psi]", "(", 
     RowBox[{"R", ",", "\[CapitalOmega]"}], ")"}]}]}], 
  TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.7634768369145517`*^9, 3.763476847200053*^9}}],

Cell[TextData[{
 "and pick either ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Alpha]", "=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"d", "-", "1"}], ")"}], "/", "2"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " or ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Alpha]", "=", "0"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " at the end of the calculation depending on what we desire."
}], "Text",
 CellChangeTimes->{{3.7634768486418962`*^9, 3.763476872359004*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{"E", "=", 
   FractionBox[
    RowBox[{
     RowBox[{
      FractionBox["1", 
       RowBox[{"2", "\[Mu]"}]], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         SubscriptBox["\[Integral]", "\[ScriptCapitalV]"], 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{
             OverscriptBox["\[Del]", "\[RightVector]"], 
             SuperscriptBox["\[CapitalPsi]", "*"]}], "\[CenterDot]", 
            RowBox[{
             OverscriptBox["\[Del]", "\[RightVector]"], "\[CapitalPsi]"}]}], 
           ")"}], 
          RowBox[{"\[DifferentialD]", "V"}]}]}], "-", 
