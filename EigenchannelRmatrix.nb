Cell[BoxData[
 FormBox[
  RowBox[{"E", "=", 
   FractionBox[
    RowBox[{
     RowBox[{
      FractionBox["1", 
       RowBox[{"2", "\[Mu]"}]], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         SubscriptBox["\[Integral]", "\[ScriptCapitalV]"], 
