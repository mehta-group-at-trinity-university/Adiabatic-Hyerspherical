 TextAlignment->Left,
 TextJustification->0.],

Cell[TextData[{
 "Here, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox[
      RowBox[{"-", "1"}], 
      RowBox[{"2", "\[Mu]"}]], 
     SuperscriptBox["\[Del]", "2"]}], "=", 
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{"-", "1"}], 
       RowBox[{"2", "\[Mu]"}]], 
      RowBox[{
       SubsuperscriptBox["\[Sum]", 
        RowBox[{"i", "=", "1"}], "d"], 
       FractionBox[
        SuperscriptBox["\[PartialD]", "2"], 
        RowBox[{"\[PartialD]", 
         SubsuperscriptBox["x", "i", "2"]}]]}]}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{"-", 
        FractionBox["1", 
