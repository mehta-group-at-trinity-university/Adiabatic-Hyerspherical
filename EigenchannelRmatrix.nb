 TextJustification->0.],

Cell[BoxData[
 FormBox[
  RowBox[{"E", "=", 
   FractionBox[
    RowBox[{
     RowBox[{
      FractionBox["1", 
       RowBox[{"2", "\[Mu]"}]], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         SubscriptBox["\[Integral]", "\[ScriptCapitalV]"], 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{
             OverscriptBox["\[Del]", "\[RightVector]"], 
             SuperscriptBox["\[CapitalPsi]", "*"]}], "\[CenterDot]", 
            RowBox[{
             OverscriptBox["\[Del]", "\[RightVector]"], "\[CapitalPsi]"}]}], 
           ")"}], 
          RowBox[{"\[DifferentialD]", "V"}]}]}], "-", 
        RowBox[{
         SubscriptBox["\[ContourIntegral]", "\[ScriptCapitalS]"], 
