 TextAlignment->Left,
 TextJustification->0.],

Cell[BoxData[
 FormBox[
  RowBox[{"E", "=", 
   FractionBox[
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{"\[CapitalPsi]", "|", 
      RowBox[{
       FractionBox[
        RowBox[{"-", "1"}], 
        RowBox[{"2", "\[Mu]"}]], 
       RowBox[{
        SuperscriptBox["\[Del]", "2"], 
        RowBox[{"+", 
         RowBox[{"V", "(", 
          RowBox[{"R", ",", "\[CapitalOmega]"}], ")"}]}]}]}], "|", 
      "\[CapitalPsi]"}], "\[RightAngleBracket]"}], 
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{"\[CapitalPsi]", "|", "\[CapitalPsi]"}], 
     "\[RightAngleBracket]"}]]}], TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.591139020466455*^9, 3.591139034758876*^9}, 
   3.591139068236343*^9, {3.59113912661205*^9, 3.591139154184558*^9}, 
   3.5911432089264383`*^9},
 TextAlignment->Left,
 TextJustification->0.],

Cell[TextData[{
 "Here, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox[
      RowBox[{"-", "1"}], 
      RowBox[{"2", "\[Mu]"}]], 
     SuperscriptBox["\[Del]", "2"]}], "=", 
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{"-", "1"}], 
       RowBox[{"2", "\[Mu]"}]], 
      RowBox[{
       SubsuperscriptBox["\[Sum]", 
        RowBox[{"i", "=", "1"}], "d"], 
       FractionBox[
        SuperscriptBox["\[PartialD]", "2"], 
        RowBox[{"\[PartialD]", 
         SubsuperscriptBox["x", "i", "2"]}]]}]}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{"-", 
        FractionBox["1", 
         RowBox[{"2", "\[Mu]", " ", 
          SuperscriptBox["R", 
           RowBox[{"d", "-", "1"}]]}]]}], 
       RowBox[{
        FractionBox["\[PartialD]", 
         RowBox[{"\[PartialD]", "R"}]], 
        RowBox[{"(", 
         RowBox[{
          SuperscriptBox["R", 
           RowBox[{"d", "-", "1"}]], 
          FractionBox["\[PartialD]", 
           RowBox[{"\[PartialD]", "R"}]]}], ")"}]}]}], "+", 
      FractionBox[
       SuperscriptBox["\[CapitalLambda]", "2"], 
       RowBox[{"2", "\[Mu]", " ", 
        SuperscriptBox["R", "2"]}]]}]}]}], TraditionalForm]]]
}], "Text",
 CellChangeTimes->{{3.591139166375505*^9, 3.591139297380949*^9}, 
   3.591143208929195*^9, {3.611576987834391*^9, 3.611576998450876*^9}},
 TextAlignment->Left,
 TextJustification->0.],

Cell[TextData[{
 "So let\[CloseCurlyQuote]s rewrite the equation in a slightly different way, \
multiplying by ",
 "\[LeftAngleBracket]\[CapitalPsi]|\[CapitalPsi]\[RightAngleBracket],",
 " pulling the ",
 Cell[BoxData[
  FormBox["E", TraditionalForm]]],
 " inside the integral and bringing it to the other side."
}], "Text",
 CellChangeTimes->{{3.591139540959651*^9, 3.591139596443679*^9}, {
   3.5911396302362013`*^9, 3.591139654748176*^9}, 3.591143208931432*^9},
 TextAlignment->Left,
 TextJustification->0.],

Cell[BoxData[
 FormBox[
  RowBox[{"0", "=", 
   RowBox[{"\[LeftAngleBracket]", 
    RowBox[{"\[CapitalPsi]", "|", 
     RowBox[{
      FractionBox[
       RowBox[{"-", "1"}], 
       RowBox[{"2", "\[Mu]"}]], 
      RowBox[{
       SuperscriptBox["\[Del]", "2"], 
       RowBox[{"+", 
